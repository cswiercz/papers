year = {2011},
eprint = {1109.4547v1},
eprinttype = {arxiv},
eprintclass = {math.NA},
month = sep,
annote = {17 pages},
read = {Yes},
rating = {0},
date-added = {2013-03-20T17:16:20GMT},
date-modified = {2013-07-01T23:06:46GMT},
abstract = {Smale's alpha-theory certifies that Newton iterations will converge quadratically to a solution of a square system of analytic functions based on the Newton residual and all higher order derivatives at the given point. Shub and Smale presented a bound for the higher order derivatives of a system of polynomial equations based in part on the degrees of the equations. For a given system of polynomial-exponential equations, we consider a related system of polynomial-exponential equations and provide a bound on the higher order derivatives of this related system. This bound yields a complete algorithm for certifying solutions to polynomial-exponential systems, which is implemented in alphaCertified. Examples are presented to demonstrate this certification algorithm.},
url = {http://arxiv.org/abs/1109.4547v1},
local-url = {file://localhost/Users/cswiercz/Dropbox/Papers2/Articles/2011/Hauenstein/2011%20Hauenstein.pdf},
